{
  "name": "Yancy",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "concurrently \"vite build\" \"tsc -p tsconfig.iframe.json\"",
    "watch": "concurrently --names vite,tsc \"vite build --watch\" \"tsc -p tsconfig.iframe.json --watch --preserveWatchOutput\"",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.2.3",
    "@iconify/svelte": "^5.1.0",
    "@mojolicious/server-starter": "^1.0.1",
    "@playwright/test": "^1.56.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tsconfig/svelte": "^5.0.5",
    "@types/node": "^24.10.0",
    "@yancyjs/tap-playwright": "^0.0.5",
    "concurrently": "^9.2.1",
    "svelte": "^5.43.5",
    "svelte-check": "^4.3.3",
    "typescript": "~5.9.3",
    "unplugin-icons": "^22.5.0",
    "vite": "^7.2.2"
  },
  "dependencies": {
    "debounce": "^3.0.0"
  }
}
